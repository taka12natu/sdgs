<h1>ユーザー一覧</h1>

	<table>
		<thead>
			<tr>
				<th>ユーザーID</th>
				<th>ユーザー名</th>
				<th></th>
			</tr>
		</thead>
		<tbody>
			<% @users.each do |user| %>
			<tr>
				<td><%= user.id %></td>
				<td><%= user.name %></td>
				<td><%= link_to "詳細", admins_user_path(user) %></td>
			</tr>
			<% end %>
		</tbody>
	</table>
